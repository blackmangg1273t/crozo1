local CurrentLanguage = "EN"

-- ============================================
-- ÿßŸÑÿ£ÿØŸàÿßÿ™ ÿßŸÑŸÖÿ≥ÿßÿπÿØÿ© ÿßŸÑŸÖÿ≠ÿ≥ŸÜÿ©
-- ============================================
local Utils = {}

function Utils.Create(class, properties)
    local obj = Instance.new(class)
    for prop, value in pairs(properties) do
        if prop == "Parent" then
            -- ÿ≥ŸÜŸÇŸàŸÖ ÿ®ÿ™ÿπŸäŸäŸÜ ÿßŸÑParent ŸÑÿßÿ≠ŸÇŸãÿß
        else
            obj[prop] = value
        end
    end
    return obj
end

function Utils.Tween(object, properties, duration, easingStyle, easingDirection)
    local tweenInfo = TweenInfo.new(
        duration or 0.3,
        easingStyle or Enum.EasingStyle.Quad,
        easingDirection or Enum.EasingDirection.Out,
        0,
        false,
        0
    )
    local tween = TweenService:Create(object, tweenInfo, properties)
    tween:Play()
    return tween
end

function Utils.CreateRoundedFrame(properties, theme)
    local frame = Utils.Create("Frame", properties)
    local corner = Utils.Create("UICorner", {
        CornerRadius = UDim.new(0, properties.CornerRadius or theme.CornerRadius.Medium)
    })
    corner.Parent = frame
    return frame
end

function Utils.CreateGlassEffect(parent, theme)
    local frame = Utils.Create("Frame", {
        Name = "GlassEffect",
        BackgroundTransparency = theme.Transparencies.Glass,
        BackgroundColor3 = theme.Colors.Glass,
        Size = UDim2.new(1, 0, 1, 0),
        ZIndex = 0
    })
    
    local uicorner = Utils.Create("UICorner", {
        CornerRadius = UDim.new(0, theme.CornerRadius.Large)
    })
    uicorner.Parent = frame
    
    local uistroke = Utils.Create("UIStroke", {
        Color = theme.Colors.Glass,
        Transparency = 0.9,
        Thickness = 1.5,
        ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    })
    uistroke.Parent = frame
    
    frame.Parent = parent
    return frame
end

function Utils.AddHoverEffect(button, theme)
    local originalTransparency = button.BackgroundTransparency
    local originalSize = button.Size
    
    button.MouseEnter:Connect(function()
        Utils.Tween(button, {
            BackgroundTransparency = math.max(originalTransparency - 0.15, 0),
            Size = originalSize + UDim2.new(0, 4, 0, 4)
        }, 0.15)
    end)
    
    button.MouseLeave:Connect(function()
        Utils.Tween(button, {
            BackgroundTransparency = originalTransparency,
            Size = originalSize
        }, 0.15)
    end)
end

function Utils.CreateBorderedFrame(properties, theme, borderStyle)
    local frame = Utils.Create("Frame", properties)
    
    local corner = Utils.Create("UICorner", {
        CornerRadius = UDim.new(0, properties.CornerRadius or theme.CornerRadius.Medium)
    })
    corner.Parent = frame
    
    local stroke = Utils.Create("UIStroke", {
        Color = theme.Colors.Border,
        Transparency = 0.3,
        Thickness = borderStyle or theme.BorderStyles.Medium,
        ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    })
    
    if borderStyle == theme.BorderStyles.Gradient then
        local gradient = Utils.Create("UIGradient", {
            Rotation = 45,
            Color = ColorSequence.new({
                ColorSequenceKeypoint.new(0, theme.Colors.Gradient1),
                ColorSequenceKeypoint.new(0.5, theme.Colors.Accent),
                ColorSequenceKeypoint.new(1, theme.Colors.Gradient2)
            })
        })
        gradient.Parent = stroke
    end
    
    stroke.Parent = frame
    return frame
end

function Utils.SaveConfig(name, data)
    local success, result = pcall(function()
        local json = HttpService:JSONEncode(data)
        if isfolder and makefolder and writefile then
            if not isfolder(CONFIG_FOLDER) then
                makefolder(CONFIG_FOLDER)
            end
            writefile(CONFIG_FOLDER .. "/" .. name .. ".json", json)
        else
            local DataStoreService = game:GetService("DataStoreService")
            local store = DataStoreService:GetDataStore(CONFIG_FOLDER)
            store:SetAsync(name, data)
        end
    end)
    return success
end

function Utils.LoadConfig(name)
    local success, result = pcall(function()
        if isfolder and readfile then
            if isfolder(CONFIG_FOLDER) then
                local path = CONFIG_FOLDER .. "/" .. name .. ".json"
                if readfile and isfile and isfile(path) then
                    local json = readfile(path)
                    return HttpService:JSONDecode(json)
                end
            end
        else
            local DataStoreService = game:GetService("DataStoreService")
            local store = DataStoreService:GetDataStore(CONFIG_FOLDER)
            return store:GetAsync(name)
        end
    end)
    return success and result or {}
end

function Utils.CreateIcon(iconName, size, color, parent)
    local iconId = ICONS[iconName] or iconName
    local icon = Utils.Create("ImageLabel", {
        Image = iconId,
        ImageColor3 = color or CurrentTheme.Colors.Text,
        BackgroundTransparency = 1,
        Size = size or UDim2.new(0, 24, 0, 24),
        ImageTransparency = 0
    })
    
    if parent then
        icon.Parent = parent
    end
    
    return icon
end

function Utils.ApplyThemeToObject(obj, theme)
    if obj:IsA("Frame") or obj:IsA("TextButton") then
        if obj.Name:find("Header") then
            obj.BackgroundColor3 = theme.Colors.Secondary
            obj.BackgroundTransparency = theme.Transparencies.Header
        elseif obj.Name:find("Button") then
            obj.BackgroundColor3 = theme.Colors.Accent
            obj.BackgroundTransparency = theme.Transparencies.Button
        elseif obj.Name:find("Card") then
            obj.BackgroundColor3 = theme.Colors.Card
            obj.BackgroundTransparency = theme.Transparencies.Card
        end
    elseif obj:IsA("TextLabel") or obj:IsA("TextButton") then
        if obj.Text then
            obj.TextColor3 = theme.Colors.Text
        end
    elseif obj:IsA("UIStroke") then
        obj.Color = theme.Colors.Border
    end
    
    for _, child in pairs(obj:GetChildren()) do
        Utils.ApplyThemeToObject(child, theme)
    end
end

-- ============================================
-- ŸÜÿ∏ÿßŸÖ ÿßŸÑÿ≠ÿ±ŸÉÿßÿ™ ÿßŸÑŸÖÿ™ŸÇÿØŸÖ
-- ============================================
local Animations = {
    Pulse = function(object)
        local originalSize = object.Size
        local tween = Utils.Tween(object, {Size = originalSize + UDim2.new(0, 6, 0, 6)}, 0.2)
        tween.Completed:Connect(function()
            Utils.Tween(object, {Size = originalSize}, 0.2)
        end)
    end,
    
    Shake = function(object)
        local originalPosition = object.Position
        for i = 1, 4 do
            Utils.Tween(object, {Position = originalPosition + UDim2.new(0, 4, 0, 0)}, 0.05)
            wait(0.05)
            Utils.Tween(object, {Position = originalPosition - UDim2.new(0, 4, 0, 0)}, 0.05)
            wait(0.05)
        end
        Utils.Tween(object, {Position = originalPosition}, 0.1)
    end,
    
    Bounce = function(object)
        local originalPosition = object.Position
        Utils.Tween(object, {Position = originalPosition - UDim2.new(0, 0, 0, 10)}, 0.1)
        wait(0.1)
        Utils.Tween(object, {Position = originalPosition}, 0.1)
    end,
    
    FadeIn = function(object)
        object.Visible = true
        Utils.Tween(object, {BackgroundTransparency = 0.1})
    end,
    
    FadeOut = function(object)
        local tween = Utils.Tween(object, {BackgroundTransparency = 1})
        tween.Completed:Connect(function()
            object.Visible = false
        end)
    end,
    
    SlideIn = function(object, direction)
        local originalPosition = object.Position
        if direction == "left" then
            object.Position = originalPosition - UDim2.new(0.2, 0, 0, 0)
        elseif direction == "right" then
            object.Position = originalPosition + UDim2.new(0.2, 0, 0, 0)
        elseif direction == "up" then
            object.Position = originalPosition - UDim2.new(0, 0, 0.2, 0)
        elseif direction == "down" then
            object.Position = originalPosition + UDim2.new(0, 0, 0.2, 0)
        end
        
        Utils.Tween(object, {Position = originalPosition}, 0.3)
    end,
    
    Glow = function(object, color)
        local originalColor = object.BackgroundColor3
        local originalTransparency = object.BackgroundTransparency
        
        Utils.Tween(object, {
            BackgroundColor3 = color or CurrentTheme.Colors.Accent,
            BackgroundTransparency = originalTransparency - 0.2
        }, 0.3)
        
        wait(0.5)
        
        Utils.Tween(object, {
            BackgroundColor3 = originalColor,
            BackgroundTransparency = originalTransparency
        }, 0.3)
    end
}

-- ============================================
-- ÿßŸÑÿØÿßŸÑÿ© ÿßŸÑÿ±ÿ¶Ÿäÿ≥Ÿäÿ© ŸÑÿ•ŸÜÿ¥ÿßÿ° ÿßŸÑŸÜÿßŸÅÿ∞ÿ©
-- ============================================
function UltimateUI:MakeWindow(config)
    config = config or {}
    
    CurrentTheme = Themes[config.Theme or "DarkRed"] or Themes.DarkRed
    CurrentLanguage = config.Language or "EN"
    
    local screenGui = Utils.Create("ScreenGui", {
        Name = SCREEN_GUI_NAME,
        DisplayOrder = 100,
        ResetOnSpawn = false,
        IgnoreGuiInset = true
    })
    
    local window = Utils.Create("Frame", {
        Name = "MainWindow",
        Size = UDim2.new(0, config.Width or 650, 0, config.Height or 550),
        Position = UDim2.new(0.5, 0, 0.5, 0),
        AnchorPoint = Vector2.new(0.5, 0.5),
        BackgroundColor3 = CurrentTheme.Colors.Primary,
        BackgroundTransparency = CurrentTheme.Transparencies.Window,
        ClipsDescendants = true,
        Active = true
    })
    
    Utils.CreateGlassEffect(window, CurrentTheme)
    
    local windowCorner = Utils.Create("UICorner", {
        CornerRadius = UDim.new(0, CurrentTheme.CornerRadius.Large)
    })
    windowCorner.Parent = window
    
    local windowStroke = Utils.CreateBorderedFrame({}, CurrentTheme, CurrentTheme.BorderStyles.Gradient)
    windowStroke.Size = UDim2.new(1, 4, 1, 4)
    windowStroke.Position = UDim2.new(0, -2, 0, -2)
    windowStroke.BackgroundTransparency = 1
    windowStroke.ZIndex = -1
    windowStroke.Parent = window
    
    -- ÿßŸÑŸáŸäÿØÿ±
    local header = Utils.Create("Frame", {
        Name = "Header",
        Size = UDim2.new(1, 0, 0, 70),
        BackgroundColor3 = CurrentTheme.Colors.Secondary,
        BackgroundTransparency = CurrentTheme.Transparencies.Header,
        BorderSizePixel = 0
    })
    
    local headerCorner = Utils.Create("UICorner", {
        CornerRadius = UDim.new(0, CurrentTheme.CornerRadius.Large, 0, 0)
    })
    headerCorner.Parent = header
    
    local titleContainer = Utils.Create("Frame", {
        Size = UDim2.new(0.7, 0, 1, 0),
        BackgroundTransparency = 1
    })
    
    local titleIcon = Utils.CreateIcon("sparkles", UDim2.new(0, 32, 0, 32), 
        CurrentTheme.Colors.Accent, titleContainer)
    titleIcon.Position = UDim2.new(0, 20, 0.5, 0)
    titleIcon.AnchorPoint = Vector2.new(0, 0.5)
    
    local title = Utils.Create("TextLabel", {
        Name = "Title",
        Text = config.Title or Languages[CurrentLanguage].Title,
        TextColor3 = CurrentTheme.Colors.Text,
        TextSize = 26,
        Font = Enum.Font.GothamBold,
        TextXAlignment = CurrentLanguage == "AR" and Enum.TextXAlignment.Right or Enum.TextXAlignment.Left,
        BackgroundTransparency = 1,
        Position = UDim2.new(0, 60, 0, 15),
        Size = UDim2.new(1, -70, 0, 30),
        TextStrokeTransparency = 0.7,
        TextStrokeColor3 = CurrentTheme.Colors.Primary
    })
    title.Parent = titleContainer
    
    local subtitle = Utils.Create("TextLabel", {
        Name = "Description",
        Text = config.SubTitle or Languages[CurrentLanguage].SubTitle,
        TextColor3 = CurrentTheme.Colors.SubText,
        TextSize = 14,
        Font = Enum.Font.Gotham,
        TextXAlignment = CurrentLanguage == "AR" and Enum.TextXAlignment.Right or Enum.TextXAlignment.Left,
        BackgroundTransparency = 1,
        Position = UDim2.new(0, 60, 0, 45),
        Size = UDim2.new(1, -70, 0, 20)
    })
    subtitle.Parent = titleContainer
    
    titleContainer.Parent = header
    
    local controls = Utils.Create("Frame", {
        Size = UDim2.new(0.3, 0, 1, 0),
        Position = UDim2.new(0.7, 0, 0, 0),
        BackgroundTransparency = 1
    })
    
    local themeBtn = Utils.Create("TextButton", {
        Text = "üé®",
        TextSize = 20,
        BackgroundColor3 = CurrentTheme.Colors.Accent,
        BackgroundTransparency = 0.7,
        Size = UDim2.new(0, 40, 0, 40),
        Position = UDim2.new(0, 10, 0.5, 0),
        AnchorPoint = Vector2.new(0, 0.5),
        AutoButtonColor = false
    })
    Utils.Create("UICorner", {CornerRadius = UDim.new(1, 0)}).Parent = themeBtn
    Utils.AddHoverEffect(themeBtn, CurrentTheme)
    themeBtn.Parent = controls
    
    local languageBtn = Utils.Create("TextButton", {
        Text = CurrentLanguage == "EN" and "üá∫üá∏" or "üá∏üá¶",
        TextSize = 20,
        BackgroundColor3 = CurrentTheme.Colors.Success,
        BackgroundTransparency = 0.7,
        Size = UDim2.new(0, 40, 0, 40),
        Position = UDim2.new(0.33, -20, 0.5, 0),
        AnchorPoint = Vector2.new(0.5, 0.5),
        AutoButtonColor = false
    })
    Utils.Create("UICorner", {CornerRadius = UDim.new(1, 0)}).Parent = languageBtn
    Utils.AddHoverEffect(languageBtn, CurrentTheme)
    languageBtn.Parent = controls
    
    local minimizeBtn = Utils.Create("TextButton", {
        Text = "‚îÄ",
        TextColor3 = CurrentTheme.Colors.Text,
        TextSize = 24,
        Font = Enum.Font.GothamBold,
        BackgroundColor3 = CurrentTheme.Colors.Accent,
        BackgroundTransparency = 0.7,
        Size = UDim2.new(0, 40, 0, 40),
        Position = UDim2.new(0.66, -20, 0.5, 0),
        AnchorPoint = Vector2.new(0.5, 0.5),
        AutoButtonColor = false
    })
    Utils.Create("UICorner", {CornerRadius = UDim.new(1, 0)}).Parent = minimizeBtn
    Utils.AddHoverEffect(minimizeBtn, CurrentTheme)
    minimizeBtn.Parent = controls
    
    local closeBtn = Utils.Create("TextButton", {
        Text = "‚úï",
        TextColor3 = CurrentTheme.Colors.Text,
        TextSize = 20,
        Font = Enum.Font.GothamBold,
        BackgroundColor3 = CurrentTheme.Colors.Error,
        BackgroundTransparency = 0.7,
        Size = UDim2.new(0, 40, 0, 40),
        Position = UDim2.new(1, -10, 0.5, 0),
        AnchorPoint = Vector2.new(1, 0.5),
        AutoButtonColor = false
    })
    Utils.Create("UICorner", {CornerRadius = UDim.new(1, 0)}).Parent = closeBtn
    Utils.AddHoverEffect(closeBtn, CurrentTheme)
    closeBtn.Parent = controls
    
    local content = Utils.Create("Frame", {
        Name = "Content",
        Size = UDim2.new(1, 0, 1, -70),
        Position = UDim2.new(0, 0, 0, 70),
        BackgroundTransparency = 1
    })
    
    local tabsContainer = Utils.Create("Frame", {
        Name = "TabsContainer",
        Size = UDim2.new(1, -40, 0, 50),
        Position = UDim2.new(0, 20, 0, 10),
        BackgroundTransparency = 1
    })
    
    local tabsListLayout = Utils.Create("UIListLayout", {
        FillDirection = Enum.FillDirection.Horizontal,
        HorizontalAlignment = CurrentLanguage == "AR" and Enum.HorizontalAlignment.Right or Enum.HorizontalAlignment.Left,
        SortOrder = Enum.SortOrder.LayoutOrder,
        Padding = UDim.new(0, 10)
    })
    tabsListLayout.Parent = tabsContainer
    
    local pagesContainer = Utils.Create("Frame", {
        Name = "PagesContainer",
        Size = UDim2.new(1, -40, 1, -80),
        Position = UDim2.new(0, 20, 0, 70),
        BackgroundTransparency = 1,
        ClipsDescendants = true
    })
    
    header.Parent = window
    content.Parent = window
    tabsContainer.Parent = content
    pagesContainer.Parent = content
    window.Parent = screenGui
    
    if Players.LocalPlayer then
        screenGui.Parent = Players.LocalPlayer:WaitForChild("PlayerGui")
    end
    
    -- Ÿàÿ∏ÿßÿ¶ŸÅ ÿßŸÑŸÜÿßŸÅÿ∞ÿ©
    local dragging = false
    local dragStart, windowStart
    
    header.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            dragStart = input.Position
            windowStart = window.Position
            Utils.Tween(window, {BackgroundTransparency = 0.02})
        end
    end)
    
    header.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = false
            Utils.Tween(window, {BackgroundTransparency = CurrentTheme.Transparencies.Window})
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
            local delta = input.Position - dragStart
            window.Position = UDim2.new(
                windowStart.X.Scale,
                windowStart.X.Offset + delta.X,
                windowStart.Y.Scale,
                windowStart.Y.Offset + delta.Y
            )
        end
    end)
    
    local isMinimized = false
    local originalSize = window.Size
    
    minimizeBtn.MouseButton1Click:Connect(function()
        Animations.Bounce(minimizeBtn)
        isMinimized = not isMinimized
        if isMinimized then
            Utils.Tween(window, {Size = UDim2.new(originalSize.X.Scale, originalSize.X.Offset, 0, 70)})
            minimizeBtn.Text = "+"
        else
            Utils.Tween(window, {Size = originalSize})
            minimizeBtn.Text = "‚îÄ"
        end
    end)
    
    local themeIndex = 1
    local themeNames = {"DarkRed", "GradientRed", "MidnightRed", "AuroraRed"}
    
    themeBtn.MouseButton1Click:Connect(function()
        Animations.Pulse(themeBtn)
        themeIndex = themeIndex + 1
        if themeIndex > #themeNames then
            themeIndex = 1
        end
        
        CurrentTheme = Themes[themeNames[themeIndex]]
        themeBtn.Text = {"üé®", "üî¥", "üåô", "üåà"}[themeIndex]
        
        Utils.ApplyThemeToObject(window, CurrentTheme)
        
        print("‚úÖ ÿ™ŸÖ ÿ™ÿ∫ŸäŸäÿ± ÿßŸÑÿ´ŸäŸÖ ÿ•ŸÑŸâ: " .. CurrentTheme.Name)
    end)
    
    languageBtn.MouseButton1Click:Connect(function()
        Animations.Shake(languageBtn)
        CurrentLanguage = CurrentLanguage == "EN" and "AR" or "EN"
        languageBtn.Text = CurrentLanguage == "EN" and "üá∫üá∏" or "üá∏üá¶"
        
        title.Text = Languages[CurrentLanguage].Title
        subtitle.Text = Languages[CurrentLanguage].SubTitle
        
        print("‚úÖ ÿ™ŸÖ ÿ™ÿ∫ŸäŸäÿ± ÿßŸÑŸÑÿ∫ÿ© ÿ•ŸÑŸâ: " .. CurrentLanguage)
    end)
    
    closeBtn.MouseButton1Click:Connect(function()
        Animations.Shake(closeBtn)
        Animations.FadeOut(window)
        wait(0.3)
        screenGui:Destroy()
    end)
    
    if RunService:IsMobile() then
        local uiScale = Utils.Create("UIScale", {Scale = 0.85})
        uiScale.Parent = window
    end
    
    local isVisible = true
    UserInputService.InputBegan:Connect(function(input, gameProcessed)
        if not gameProcessed and input.KeyCode == Enum.KeyCode.RightShift then
            isVisible = not isVisible
            screenGui.Enabled = isVisible
            if isVisible then
                Animations.FadeIn(window)
            else
                Animations.FadeOut(window)
            end
        end
    end)
    
    local windowObj = {
        Gui = screenGui,
        Window = window,
        Content = content,
        TabsContainer = tabsContainer,
        PagesContainer = pagesContainer,
        Language = CurrentLanguage,
        SaveFolder = config.SaveFolder or "PremiumConfig.json",
        CurrentTheme = CurrentTheme,
        Flags = {},
        Config = {},
        Tabs = {}
    }
    
    spawn(function()
        wait(0.5)
        local savedConfig = Utils.LoadConfig(windowObj.SaveFolder)
        if savedConfig then
            windowObj.Config = savedConfig
        end
    end)
    
    window.Destroying:Connect(function()
        Utils.SaveConfig(windowObj.SaveFolder, windowObj.Config)
    end)
    
    function windowObj:AddTab(name, iconName)
        local tabButton = Utils.Create("TextButton", {
            Name = name,
            Text = name,
            TextColor3 = CurrentTheme.Colors.Text,
            TextSize = 15,
            Font = Enum.Font.GothamSemibold,
            BackgroundColor3 = CurrentTheme.Colors.Secondary,
            BackgroundTransparency = 0.8,
            Size = UDim2.new(0, 120, 1, 0),
            AutoButtonColor = false,
            LayoutOrder = #self.Tabs + 1
        })
        
        Utils.Create("UICorner", {
            CornerRadius = UDim.new(0, CurrentTheme.CornerRadius.Small)
        }).Parent = tabButton
        
        local buttonStroke = Utils.Create("UIStroke", {
            Color = CurrentTheme.Colors.Accent,
            Transparency = 0.8,
            Thickness = 1.5
        })
        buttonStroke.Parent = tabButton
        
        if iconName then
            local tabIcon = Utils.CreateIcon(iconName, UDim2.new(0, 20, 0, 20), 
                CurrentTheme.Colors.Text, tabButton)
            tabIcon.Position = UDim2.new(0, 15, 0.5, 0)
            tabIcon.AnchorPoint = Vector2.new(0, 0.5)
        end
        
        Utils.AddHoverEffect(tabButton, CurrentTheme)
        
        local page = Utils.Create("ScrollingFrame", {
            Name = name .. "_Page",
            Size = UDim2.new(1, 0, 1, 0),
            BackgroundTransparency = 1,
            ScrollBarThickness = 5,
            ScrollBarImageColor3 = CurrentTheme.Colors.Accent,
            ScrollBarImageTransparency = 0.6,
            CanvasSize = UDim2.new(0, 0, 0, 0),
            Visible = false
        })
        
        local pageListLayout = Utils.Create("UIListLayout", {
            SortOrder = Enum.SortOrder.LayoutOrder,
            Padding = UDim.new(0, 15)
        })
        pageListLayout.Parent = page
        
        pageListLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
            page.CanvasSize = UDim2.new(0, 0, 0, pageListLayout.AbsoluteContentSize.Y + 20)
        end)
        
        local function selectTab()
            for _, existingPage in pairs(self.PagesContainer:GetChildren()) do
                if existingPage:IsA("ScrollingFrame") then
                    existingPage.Visible = false
                end
            end
            
            for _, existingTab in pairs(self.TabsContainer:GetChildren()) do
                if existingTab:IsA("TextButton") then
                    Utils.Tween(existingTab, {BackgroundTransparency = 0.8})
                    Utils.Tween(existingTab.UIStroke, {Transparency = 0.8})
                end
            end
            
            page.Visible = true
            Utils.Tween(tabButton, {BackgroundTransparency = 0.4})
            Utils.Tween(tabButton.UIStroke, {Transparency = 0.2})
            Animations.Pulse(tabButton)
        end
        
        tabButton.MouseButton1Click:Connect(selectTab)
        
        tabButton.Parent = self.TabsContainer
        page.Parent = self.PagesContainer
        
        if #self.TabsContainer:GetChildren() == 1 then
            selectTab()
        end
        
        local tabObj = {
            Button = tabButton,
            Page = page,
            Window = self,
            Elements = {}
        }
        
        function tabObj:AddSection(name, iconName)
            local section = Utils.Create("Frame", {
                Name = "Section_" .. name,
                Size = UDim2.new(1, 0, 0, 0),
                BackgroundColor3 = CurrentTheme.Colors.Card,
                BackgroundTransparency = CurrentTheme.Transparencies.Card,
                LayoutOrder = #self.Page:GetChildren() + 1
            })
            
            local corner = Utils.Create("UICorner", {
                CornerRadius = UDim.new(0, CurrentTheme.CornerRadius.Medium)
            })
            corner.Parent = section
            
            local stroke = Utils.CreateBorderedFrame({}, CurrentTheme, CurrentTheme.BorderStyles.Thin)
            stroke.Size = UDim2.new(1, 0, 1, 0)
            stroke.BackgroundTransparency = 1
            stroke.Parent = section
            
            local header = Utils.Create("Frame", {
                Size = UDim2.new(1, 0, 0, 50),
                BackgroundTransparency = 1
            })
            
            if iconName then
                local sectionIcon = Utils.CreateIcon(iconName, UDim2.new(0, 24, 0, 24), 
                    CurrentTheme.Colors.Accent, header)
                sectionIcon.Position = UDim2.new(0, 15, 0.5, 0)
                sectionIcon.AnchorPoint = Vector2.new(0, 0.5)
            end
            
            local title = Utils.Create("TextLabel", {
                Text = name,
                TextColor3 = CurrentTheme.Colors.Text,
                TextSize = 18,
                Font = Enum.Font.GothamBold,
                TextXAlignment = CurrentLanguage == "AR" and Enum.TextXAlignment.Right or Enum.TextXAlignment.Left,
                BackgroundTransparency = 1,
                Position = UDim2.new(0, iconName and 50 or 15, 0, 0),
                Size = UDim2.new(1, iconName and -60 or -30, 1, 0)
            })
            title.Parent = header
            
            local divider = Utils.Create("Frame", {
                Size = UDim2.new(1, -30, 0, 1),
                Position = UDim2.new(0, 15, 1, 0),
                BackgroundColor3 = CurrentTheme.Colors.Accent,
                BackgroundTransparency = 0.7
            })
            divider.Parent = header
            
            local container = Utils.Create("Frame", {
                Name = "Container",
                Size = UDim2.new(1, -20, 1, -60),
                Position = UDim2.new(0, 10, 0, 60),
                BackgroundTransparency = 1
            })
            
            local listLayout = Utils.Create("UIListLayout", {
                SortOrder = Enum.SortOrder.LayoutOrder,
                Padding = UDim.new(0, 12)
            })
            listLayout.Parent = container
            
            listLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
                section.Size = UDim2.new(1, 0, 0, listLayout.AbsoluteContentSize.Y + 70)
            end)
            
            header.Parent = section
            container.Parent = section
            section.Parent = self.Page
            
            local sectionObj = {
                Frame = section,
                Container = container,
                Tab = self,
                CurrentTheme = CurrentTheme
            }
            
            -- ÿØÿßŸÑÿ© ÿ•ŸÜÿ¥ÿßÿ° Toggle ŸÖÿ≠ÿ≥ŸÜ
            function sectionObj:AddToggle(config)
                config = config or {}
                
                local toggle = Utils.Create("Frame", {
                    Name = "Toggle_" .. config.Title,
                    Size = UDim2.new(1, 0, 0, 50),
                    BackgroundTransparency = 1,
                    LayoutOrder = #self.Container:GetChildren() + 1
                })
                
                local toggleIcon = Utils.CreateIcon("toggleleft", 
                    UDim2.new(0, 24, 0, 24), CurrentTheme.Colors.SubText, toggle)
                toggleIcon.Position = UDim2.new(0, 10, 0.5, 0)
                toggleIcon.AnchorPoint = Vector2.new(0, 0.5)
                
                local label = Utils.Create("TextLabel", {
                    Name = "Label",
                    Text = config.Title or Languages[CurrentLanguage].Toggle,
                    TextColor3 = CurrentTheme.Colors.Text,
                    TextSize = 15,
                    Font = Enum.Font.GothamSemibold,
                    TextXAlignment = CurrentLanguage == "AR" and Enum.TextXAlignment.Right or Enum.TextXAlignment.Left,
                    BackgroundTransparency = 1,
                    Position = UDim2.new(0, 44, 0, 0),
                    Size = UDim2.new(0.6, -44, 1, 0)
                })
                label.Parent = toggle
                
                local switch = Utils.Create("Frame", {
                    Name = "Switch",
                    Size = UDim2.new(0, 60, 0, 32),
                    Position = UDim2.new(1, -10, 0.5, 0),
                    AnchorPoint = Vector2.new(1, 0.5),
                    BackgroundColor3 = CurrentTheme.Colors.Secondary,
                    BackgroundTransparency = 0.3
                })
                
                Utils.Create("UICorner", {
                    CornerRadius = UDim.new(0, CurrentTheme.CornerRadius.Toggle)
                }).Parent = switch
                
                local switchStroke = Utils.Create("UIStroke", {
                    Color = CurrentTheme.Colors.Accent,
                    Transparency = 0.5,
                    Thickness = 2
                })
                switchStroke.Parent = switch
                
                local toggleCircle = Utils.Create("Frame", {
                    Name = "ToggleCircle",
                    Size = UDim2.new(0, 26, 0, 26),
                    Position = UDim2.new(0, 3, 0.5, 0),
                    AnchorPoint = Vector2.new(0, 0.5),
                    BackgroundColor3 = CurrentTheme.Colors.Text,
                    BackgroundTransparency = 0
                })
                
                Utils.Create("UICorner", {
                    CornerRadius = UDim.new(1, 0)
                }).Parent = toggleCircle
                
                local state = config.Default or false
                local flagName = config.Flag or config.Title
                
                if flagName then
                    self.Tab.Window.Flags[flagName] = state
                end
                
                local function updateToggle()
                    if state then
                        Utils.Tween(switch, {BackgroundColor3 = CurrentTheme.Colors.Success})
                        Utils.Tween(toggleCircle, {Position = UDim2.new(1, -29, 0.5, 0)})
                        Utils.Tween(toggleIcon, {ImageColor3 = CurrentTheme.Colors.Success})
                        toggleIcon.Image = ICONS["toggleright"]
                    else
                        Utils.Tween(switch, {BackgroundColor3 = CurrentTheme.Colors.Secondary})
                        Utils.Tween(toggleCircle, {Position = UDim2.new(0, 3, 0.5, 0)})
                        Utils.Tween(toggleIcon, {ImageColor3 = CurrentTheme.Colors.SubText})
                        toggleIcon.Image = ICONS["toggleleft"]
                    end
                    
                    if flagName then
                        self.Tab.Window.Flags[flagName] = state
                        self.Tab.Window.Config[flagName] = state
                    end
                    
                    if config.Callback then
                        config.Callback(state)
                    end
                end
                
                local interactive = Utils.Create("TextButton", {
                    Size = UDim2.new(1, 0, 1, 0),
                    BackgroundTransparency = 1,
                    Text = "",
                    AutoButtonColor = false
                })
                
                interactive.MouseButton1Click:Connect(function()
                    state = not state
                    updateToggle()
                    Animations.Pulse(switch)
                end)
                
                Utils.AddHoverEffect(switch, CurrentTheme)
                toggleCircle.Parent = switch
                switch.Parent = toggle
                interactive.Parent = toggle
                toggle.Parent = self.Container
                
                if flagName and self.Tab.Window.Config[flagName] ~= nil then
                    state = self.Tab.Window.Config[flagName]
                end
                
                updateToggle()
                
                return {
                    Frame = toggle,
                    GetState = function() return state end,
                    SetState = function(newState)
                        state = newState
                        updateToggle()
                    end
                }
            end
            
            -- ÿØÿßŸÑÿ© ÿ•ŸÜÿ¥ÿßÿ° Slider ŸÖÿ≠ÿ≥ŸÜ
            function sectionObj:AddSlider(config)
                config = config or {}
                
                local slider = Utils.Create("Frame", {
                    Name = "Slider_" .. config.Title,
                    Size = UDim2.new(1, 0, 0, 75),
                    BackgroundTransparency = 1,
                    LayoutOrder = #self.Container:GetChildren() + 1
                })
                
                local header = Utils.Create("Frame", {
                    Size = UDim2.new(1, 0, 0, 30),
                    BackgroundTransparency = 1
                })
                
                local sliderIcon = Utils.CreateIcon("slidershorizontal", 
                    UDim2.new(0, 24, 0, 24), CurrentTheme.Colors.SubText, header)
                sliderIcon.Position = UDim2.new(0, 10, 0.5, 0)
                sliderIcon.AnchorPoint = Vector2.new(0, 0.5)
                
                local label = Utils.Create("TextLabel", {
                    Name = "Label",
                    Text = config.Title or Languages[CurrentLanguage].Slider,
                    TextColor3 = CurrentTheme.Colors.Text,
                    TextSize = 15,
                    Font = Enum.Font.GothamSemibold,
                    TextXAlignment = CurrentLanguage == "AR" and Enum.TextXAlignment.Right or Enum.TextXAlignment.Left,
                    BackgroundTransparency = 1,
                    Position = UDim2.new(0, 44, 0, 0),
                    Size = UDim2.new(0.5, -44, 1, 0)
                })
                label.Parent = header
                
                local valueLabel = Utils.Create("TextLabel", {
                    Name = "Value",
                    Text = tostring(config.Default or config.Min or 0),
                    TextColor3 = CurrentTheme.Colors.Accent,
                    TextSize = 15,
                    Font = Enum.Font.GothamBold,
                    TextXAlignment = Enum.TextXAlignment.Right,
                    BackgroundTransparency = 1,
                    Size = UDim2.new(0.5, -10, 1, 0),
                    Position = UDim2.new(0.5, 0, 0, 0)
                })
                valueLabel.Parent = header
                header.Parent = slider
                
                local track = Utils.Create("Frame", {
                    Name = "Track",
                    Size = UDim2.new(1, -20, 0, 10),
                    Position = UDim2.new(0, 10, 0, 50),
                    BackgroundColor3 = CurrentTheme.Colors.Secondary,
                    BackgroundTransparency = 0.4
                })
                
                Utils.Create("UICorner", {
                    CornerRadius = UDim.new(0, CurrentTheme.CornerRadius.Slider)
                }).Parent = track
                
                local trackStroke = Utils.Create("UIStroke", {
                    Color = CurrentTheme.Colors.Accent,
                    Transparency = 0.6,
                    Thickness = 1.5
                })
                trackStroke.Parent = track
                
                local fill = Utils.Create("Frame", {
                    Name = "Fill",
                    Size = UDim2.new(0, 0, 1, 0),
                    BackgroundColor3 = CurrentTheme.Colors.Accent,
                    BackgroundTransparency = 0.2
                })
                
                Utils.Create("UICorner", {
                    CornerRadius = UDim.new(0, CurrentTheme.CornerRadius.Slider)
                }).Parent = fill
                
                local thumb = Utils.Create("Frame", {
                    Name = "Thumb",
                    Size = UDim2.new(0, 26, 0, 26),
                    Position = UDim2.new(0, -13, 0.5, 0),
                    AnchorPoint = Vector2.new(0, 0.5),
                    BackgroundColor3 = CurrentTheme.Colors.Text,
                    ZIndex = 2
                })
                
                Utils.Create("UICorner", {
                    CornerRadius = UDim.new(1, 0)
                }).Parent = thumb
                
                local thumbStroke = Utils.Create("UIStroke", {
                    Color = CurrentTheme.Colors.Accent,
                    Thickness = 3,
                    Transparency = 0
                })
                thumbStroke.Parent = thumb
                
                local min = config.Min or 0
                local max = config.Max or 100
                local value = config.Default or min
                local flagName = config.Flag or config.Title
                local dragging = false
                
                if flagName then
                    self.Tab.Window.Flags[flagName] = value
                end
                
                local function updateSlider(mouseX)
                    local absolutePosition = track.AbsolutePosition.X
                    local absoluteSize = track.AbsoluteSize.X
                    
                    local relativeX = math.clamp(mouseX - absolutePosition, 0, absoluteSize)
                    local percentage = relativeX / absoluteSize
                    
                    value = math.floor(min + (max - min) * percentage)
                    valueLabel.Text = tostring(value)
                    
                    fill.Size = UDim2.new(percentage, 0, 1, 0)
                    thumb.Position = UDim2.new(percentage, -13, 0.5, 0)
                    
                    if flagName then
                        self.Tab.Window.Flags[flagName] = value
                        self.Tab.Window.Config[flagName] = value
                    end
                    
                    if config.Callback then
                        config.Callback(value)
                    end
                end
                
                thumb.InputBegan:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseButton1 then
                        dragging = true
                        Utils.Tween(thumb, {Size = UDim2.new(0, 30, 0, 30)})
                    end
                end)
                
                thumb.InputEnded:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseButton1 then
                        dragging = false
                        Utils.Tween(thumb, {Size = UDim2.new(0, 26, 0, 26)})
                        Animations.Bounce(thumb)
                    end
                end)
                
                track.InputBegan:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseButton1 then
                        updateSlider(input.Position.X)
                        Animations.Pulse(thumb)
                    end
                end)
                
                UserInputService.InputChanged:Connect(function(input)
                    if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
                        updateSlider(input.Position.X)
                    end
                end)
                
                fill.Parent = track
                thumb.Parent = track
                track.Parent = slider
                slider.Parent = self.Container
                
                if flagName and self.Tab.Window.Config[flagName] ~= nil then
                    value = self.Tab.Window.Config[flagName]
                end
                
                updateSlider(track.AbsolutePosition.X + (track.AbsoluteSize.X * ((value - min) / (max - min))))
                
                return {
                    Frame = slider,
                    GetValue = function() return value end,
                    SetValue = function(newValue)
                        value = math.clamp(newValue, min, max)
                        updateSlider(track.AbsolutePosition.X + (track.AbsoluteSize.X * ((value - min) / (max - min))))
                    end
                }
            end
            
            -- ÿØÿßŸÑÿ© ÿ•ŸÜÿ¥ÿßÿ° Dropdown ŸÖÿ≠ÿ≥ŸÜ
            function sectionObj:AddDropdown(config)
                config = config or {}
                
                local dropdown = Utils.Create("Frame", {
                    Name = "Dropdown_" .. config.Title,
                    Size = UDim2.new(1, 0, 0, 50),
                    BackgroundTransparency = 1,
                    LayoutOrder = #self.Container:GetChildren() + 1
                })
                
                local dropdownIcon = Utils.CreateIcon("list", 
                    UDim2.new(0, 24, 0, 24), CurrentTheme.Colors.SubText, dropdown)
                dropdownIcon.Position = UDim2.new(0, 10, 0.5, 0)
                dropdownIcon.AnchorPoint = Vector2.new(0, 0.5)
                
                local label = Utils.Create("TextLabel", {
                    Name = "Label",
                    Text = config.Title or Languages[CurrentLanguage].Dropdown,
                    TextColor3 = CurrentTheme.Colors.Text,
                    TextSize = 15,
                    Font = Enum.Font.GothamSemibold,
                    TextXAlignment = CurrentLanguage == "AR" and Enum.TextXAlignment.Right or Enum.TextXAlignment.Left,
                    BackgroundTransparency = 1,
                    Position = UDim2.new(0, 44, 0, 0),
                    Size = UDim2.new(0.6, -44, 1, 0)
                })
                label.Parent = dropdown
                
                local button = Utils.Create("TextButton", {
                    Name = "Button",
                    Text = config.Default or Languages[CurrentLanguage].Select,
                    TextColor3 = CurrentTheme.Colors.Text,
                    TextSize = 14,
                    Font = Enum.Font.GothamSemibold,
                    BackgroundColor3 = CurrentTheme.Colors.Secondary,
                    BackgroundTransparency = 0.3,
                    Size = UDim2.new(0.4, -10, 1, 0),
                    Position = UDim2.new(0.6, 0, 0, 0),
                    AutoButtonColor = false
                })
                
                Utils.Create("UICorner", {
                    CornerRadius = UDim.new(0, CurrentTheme.CornerRadius.Dropdown)
                }).Parent = button
                
                local arrow = Utils.CreateIcon("chevrondown", UDim2.new(0, 18, 0, 18), 
                    CurrentTheme.Colors.Text, button)
                arrow.ImageTransparency = 0.5
                arrow.Position = UDim2.new(1, -22, 0.5, 0)
                arrow.AnchorPoint = Vector2.new(1, 0.5)
                
                local list = Utils.Create("ScrollingFrame", {
                    Name = "List",
                    Size = UDim2.new(0.4, -10, 0, 0),
                    Position = UDim2.new(0.6, 0, 1, 5),
                    BackgroundColor3 = CurrentTheme.Colors.Primary,
                    BackgroundTransparency = 0.1,
                    ScrollBarThickness = 4,
                    ScrollBarImageColor3 = CurrentTheme.Colors.Accent,
                    CanvasSize = UDim2.new(0, 0, 0, 0),
                    Visible = false,
                    ZIndex = 10
                })
                
                Utils.Create("UICorner", {
                    CornerRadius = UDim.new(0, CurrentTheme.CornerRadius.Dropdown)
                }).Parent = list
                
                local listLayout = Utils.Create("UIListLayout", {
                    SortOrder = Enum.SortOrder.LayoutOrder
                })
                listLayout.Parent = list
                
                local options = config.Options or {}
                local selected = config.Default or options[1]
                local flagName = config.Flag or config.Title
                local isOpen = false
                
                if flagName then
                    self.Tab.Window.Flags[flagName] = selected
                end
                
                local function toggleList()
                    isOpen = not isOpen
                    list.Visible = isOpen
                    if isOpen then
                        Utils.Tween(list, {Size = UDim2.new(0.4, -10, 0, math.min(#options * 35, 175))})
                        Utils.Tween(arrow, {Rotation = 180})
                        arrow.Image = ICONS["chevronup"]
                        Animations.SlideIn(list, "down")
                    else
                        Utils.Tween(list, {Size = UDim2.new(0.4, -10, 0, 0)})
                        Utils.Tween(arrow, {Rotation = 0})
                        arrow.Image = ICONS["chevrondown"]
                    end
                end
                
                local function selectOption(option)
                    selected = option
                    button.Text = option
                    toggleList()
                    
                    if flagName then
                        self.Tab.Window.Flags[flagName] = selected
                        self.Tab.Window.Config[flagName] = selected
                    end
                    
                    if config.Callback then
                        config.Callback(option)
                    end
                end
                
                for i, option in ipairs(options) do
                    local optionButton = Utils.Create("TextButton", {
                        Text = option,
                        TextColor3 = CurrentTheme.Colors.Text,
                        TextSize = 13,
                        Font = Enum.Font.GothamSemibold,
                        BackgroundColor3 = CurrentTheme.Colors.Secondary,
                        BackgroundTransparency = 0.8,
                        Size = UDim2.new(1, -10, 0, 32),
                        Position = UDim2.new(0, 5, 0, (i-1)*35),
                        AutoButtonColor = false,
                        ZIndex = 11,
                        LayoutOrder = i
                    })
                    
                    Utils.Create("UICorner", {
                        CornerRadius = UDim.new(0, CurrentTheme.CornerRadius.Small)
                    }).Parent = optionButton
                    
                    optionButton.MouseButton1Click:Connect(function()
                        selectOption(option)
                        Animations.Pulse(optionButton)
                    end)
                    
                    optionButton.MouseEnter:Connect(function()
                        Utils.Tween(optionButton, {BackgroundTransparency = 0.6})
                        Animations.Glow(optionButton, CurrentTheme.Colors.Accent)
                    end)
                    
                    optionButton.MouseLeave:Connect(function()
                        Utils.Tween(optionButton, {BackgroundTransparency = 0.8})
                    end)
                    
                    optionButton.Parent = list
                end
                
                listLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
                    list.CanvasSize = UDim2.new(0, 0, 0, listLayout.AbsoluteContentSize.Y)
                end)
                
                button.MouseButton1Click:Connect(toggleList)
                Utils.AddHoverEffect(button, CurrentTheme)
                
                button.Parent = dropdown
                list.Parent = dropdown
                dropdown.Parent = self.Container
                
                if flagName and self.Tab.Window.Config[flagName] then
                    selected = self.Tab.Window.Config[flagName]
                    button.Text = selected
                end
                
                return {
                    Frame = dropdown,
                    GetSelected = function() return selected end,
                    SetSelected = function(option)
                        selected = option
                        button.Text = option
                        if config.Callback then
                            config.Callback(option)
                        end
                    end
                }
            end
            
            -- ÿØÿßŸÑÿ© ÿ•ŸÜÿ¥ÿßÿ° Social Media
            function sectionObj:AddSocialMedia(config)
                config = config or {}
                
                local social = Utils.Create("Frame", {
                    Name = "SocialMedia_" .. (config.Platform or "Custom"),
                    Size = UDim2.new(1, 0, 0, 70),
                    BackgroundColor3 = CurrentTheme.Colors.Card,
                    BackgroundTransparency = CurrentTheme.Transparencies.Social,
                    LayoutOrder = #self.Container:GetChildren() + 1
                })
                
                Utils.Create("UICorner", {
                    CornerRadius = UDim.new(0, CurrentTheme.CornerRadius.Social)
                }).Parent = social
                
                local stroke = Utils.CreateBorderedFrame({}, CurrentTheme, CurrentTheme.BorderStyles.Medium)
                stroke.Size = UDim2.new(1, 0, 1, 0)
                stroke.BackgroundTransparency = 1
                stroke.Parent = social
                
                local platformIcon = Utils.CreateIcon(
                    SOCIAL_ICONS[config.Platform] or "messagecircle",
                    UDim2.new(0, 42, 0, 42),
                    CurrentTheme.Colors.Accent,
                    social
                )
                platformIcon.Position = UDim2.new(0, 15, 0.5, 0)
                platformIcon.AnchorPoint = Vector2.new(0, 0.5)
                
                local infoFrame = Utils.Create("Frame", {
                    Size = UDim2.new(0.5, -60, 1, -20),
                    Position = UDim2.new(0, 67, 0, 10),
                    BackgroundTransparency = 1
                })
                
                local platformName = Utils.Create("TextLabel", {
                    Text = config.Platform or "Social Media",
                    TextColor3 = CurrentTheme.Colors.Text,
                    TextSize = 16,
                    Font = Enum.Font.GothamBold,
                    TextXAlignment = CurrentLanguage == "AR" and Enum.TextXAlignment.Right or Enum.TextXAlignment.Left,
                    BackgroundTransparency = 1,
                    Size = UDim2.new(1, 0, 0, 22)
                })
                platformName.Parent = infoFrame
                
                local platformDesc = Utils.Create("TextLabel", {
                    Text = config.Description or "Join our community",
                    TextColor3 = CurrentTheme.Colors.SubText,
                    TextSize = 13,
                    Font = Enum.Font.Gotham,
                    TextXAlignment = CurrentLanguage == "AR" and Enum.TextXAlignment.Right or Enum.TextXAlignment.Left,
                    BackgroundTransparency = 1,
                    Position = UDim2.new(0, 0, 0, 24),
                    Size = UDim2.new(1, 0, 0, 20)
                })
                platformDesc.Parent = infoFrame
                
                infoFrame.Parent = social
                
                local actionsFrame = Utils.Create("Frame", {
                    Size = UDim2.new(0.3, -10, 1, -20),
                    Position = UDim2.new(0.7, 0, 0, 10),
                    BackgroundTransparency = 1
                })
                
                local copyBtn = Utils.Create("TextButton", {
                    Text = Languages[CurrentLanguage].Copy,
                    TextColor3 = CurrentTheme.Colors.Text,
                    TextSize = 13,
                    Font = Enum.Font.GothamSemibold,
                    BackgroundColor3 = CurrentTheme.Colors.Accent,
                    BackgroundTransparency = 0.3,
                    Size = UDim2.new(1, 0, 0, 28),
                    Position = UDim2.new(0, 0, 0, 0),
                    AutoButtonColor = false
                })
                
                Utils.Create("UICorner", {
                    CornerRadius = UDim.new(0, CurrentTheme.CornerRadius.Small)
                }).Parent = copyBtn
                Utils.AddHoverEffect(copyBtn, CurrentTheme)
                
                copyBtn.MouseButton1Click:Connect(function()
                    if config.Link then
                        setclipboard(config.Link)
                        Animations.Pulse(copyBtn)
                        copyBtn.Text = "Copied!"
                        wait(1.5)
                        copyBtn.Text = Languages[CurrentLanguage].Copy
                    end
                end)
                
                local visitBtn = Utils.Create("TextButton", {
                    Text = Languages[CurrentLanguage].Visit,
                    TextColor3 = CurrentTheme.Colors.Text,
                    TextSize = 13,
                    Font = Enum.Font.GothamSemibold,
                    BackgroundColor3 = CurrentTheme.Colors.Success,
                    BackgroundTransparency = 0.3,
                    Size = UDim2.new(1, 0, 0, 28),
                    Position = UDim2.new(0, 0, 0, 33),
                    AutoButtonColor = false
                })
                
                Utils.Create("UICorner", {
                    CornerRadius = UDim.new(0, CurrentTheme.CornerRadius.Small)
                }).Parent = visitBtn
                Utils.AddHoverEffect(visitBtn, CurrentTheme)
                
                visitBtn.MouseButton1Click:Connect(function()
                    if config.Link then
                        print("Visiting:", config.Link)
                    end
                end)
                
                copyBtn.Parent = actionsFrame
                visitBtn.Parent = actionsFrame
                actionsFrame.Parent = social
                social.Parent = self.Container
                
                return {
                    Frame = social,
                    UpdateLink = function(newLink)
                        if config then
                            config.Link = newLink
                        end
                    end,
                    UpdateDescription = function(newDesc)
                        platformDesc.Text = newDesc
                    end
                }
            end
            
            -- ÿØÿßŸÑÿ© ÿ•ŸÜÿ¥ÿßÿ° ÿ•ÿπŸÑÿßŸÜ
            function sectionObj:AddAdvertisement(config)
                config = config or {}
                
                local ad = Utils.Create("Frame", {
                    Name = "Advertisement_" .. (config.Title or "Ad"),
                    Size = UDim2.new(1, 0, 0, 100),
                    BackgroundColor3 = CurrentTheme.Colors.Card,
                    BackgroundTransparency = CurrentTheme.Transparencies.Ad,
                    LayoutOrder = #self.Container:GetChildren() + 1
                })
                
                Utils.Create("UICorner", {
                    CornerRadius = UDim.new(0, CurrentTheme.CornerRadius.Ad)
                }).Parent = ad
                
                local borderFrame = Utils.CreateBorderedFrame({}, CurrentTheme, CurrentTheme.BorderStyles.Gradient)
                borderFrame.Size = UDim2.new(1, 4, 1, 4)
                borderFrame.Position = UDim2.new(0, -2, 0, -2)
                borderFrame.BackgroundTransparency = 1
                borderFrame.ZIndex = -1
                borderFrame.Parent = ad
                
                local gradientBg = Utils.Create("Frame", {
                    Size = UDim2.new(1, 0, 1, 0),
                    BackgroundTransparency = 0.9,
                    ZIndex = 0
                })
                
                local gradient = Utils.Create("UIGradient", {
                    Rotation = 0,
                    Color = ColorSequence.new({
                        ColorSequenceKeypoint.new(0, CurrentTheme.Colors.Gradient1),
                        ColorSequenceKeypoint.new(0.5, CurrentTheme.Colors.Accent),
                        ColorSequenceKeypoint.new(1, CurrentTheme.Colors.Gradient2)
                    }),
                    Transparency = NumberSequence.new({
                        NumberSequenceKeypoint.new(0, 0.8),
                        NumberSequenceKeypoint.new(0.5, 0.7),
                        NumberSequenceKeypoint.new(1, 0.8)
                    })
                })
                gradient.Parent = gradientBg
                
                spawn(function()
                    while ad.Parent do
                        for i = 0, 360, 2 do
                            if not ad.Parent then break end
                            gradient.Rotation = i
                            wait(0.03)
                        end
                    end
                end)
                
                gradientBg.Parent = ad
                
                local contentFrame = Utils.Create("Frame", {
                    Size = UDim2.new(1, -20, 1, -20),
                    Position = UDim2.new(0, 10, 0, 10),
                    BackgroundTransparency = 1,
                    ZIndex = 1
                })
                
                local title = Utils.Create("TextLabel", {
                    Text = config.Title or Languages[CurrentLanguage].Ads,
                    TextColor3 = CurrentTheme.Colors.Text,
                    TextSize = 18,
                    Font = Enum.Font.GothamBold,
                    TextXAlignment = Enum.TextXAlignment.Center,
                    BackgroundTransparency = 1,
                    Position = UDim2.new(0, 0, 0, 10),
                    Size = UDim2.new(1, 0, 0, 25),
                    TextStrokeTransparency = 0.8,
                    TextStrokeColor3 = CurrentTheme.Colors.Primary
                })
                title.Parent = contentFrame
                
                local description = Utils.Create("TextLabel", {
                    Text = config.Description or "Sponsored content",
                    TextColor3 = CurrentTheme.Colors.SubText,
                    TextSize = 14,
                    Font = Enum.Font.Gotham,
                    TextXAlignment = Enum.TextXAlignment.Center,
                    BackgroundTransparency = 1,
                    Position = UDim2.new(0, 0, 0, 35),
                    Size = UDim2.new(1, 0, 0, 20)
                })
                description.Parent = contentFrame
                
                local visitBtn = Utils.Create("TextButton", {
                    Text = config.ButtonText or "Visit Website",
                    TextColor3 = CurrentTheme.Colors.Accent,
                    TextSize = 13,
                    Font = Enum.Font.GothamSemibold,
                    BackgroundTransparency = 1,
                    Position = UDim2.new(0, 0, 0, 70),
                    Size = UDim2.new(1, 0, 0, 20),
                    AutoButtonColor = false
                })
                
                visitBtn.MouseButton1Click:Connect(function()
                    if config.Link then
                        print("Opening advertisement:", config.Link)
                    end
                end)
                
                Utils.AddHoverEffect(visitBtn, CurrentTheme)
                visitBtn.Parent = contentFrame
                contentFrame.Parent = ad
                ad.Parent = self.Container
                
                return {
                    Frame = ad,
                    UpdateContent = function(newConfig)
                        if newConfig.Title then title.Text = newConfig.Title end
                        if newConfig.Description then description.Text = newConfig.Description end
                        if newConfig.ButtonText then visitBtn.Text = newConfig.ButtonText end
                    end
                }
            end
            
            -- ÿØÿßŸÑÿ© ÿ•ŸÜÿ¥ÿßÿ° ÿ≤ÿ±
            function sectionObj:AddButton(config)
                config = config or {}
                
                local button = Utils.Create("TextButton", {
                    Name = "Button_" .. config.Title,
                    Text = config.Title or Languages[CurrentLanguage].Button,
                    TextColor3 = CurrentTheme.Colors.Text,
                    TextSize = 15,
                    Font = Enum.Font.GothamSemibold,
                    BackgroundColor3 = CurrentTheme.Colors.Accent,
                    BackgroundTransparency = CurrentTheme.Transparencies.Button,
                    Size = UDim2.new(1, 0, 0, 45),
                    LayoutOrder = #self.Container:GetChildren() + 1,
                    AutoButtonColor = false
                })
                
                Utils.Create("UICorner", {
                    CornerRadius = UDim.new(0, CurrentTheme.CornerRadius.Small)
                }).Parent = button
                
                local buttonStroke = Utils.Create("UIStroke", {
                    Color = CurrentTheme.Colors.Text,
                    Transparency = 0.5,
                    Thickness = 1
                })
                buttonStroke.Parent = button
                
                Utils.AddHoverEffect(button, CurrentTheme)
                
                button.MouseEnter:Connect(function()
                    Utils.Tween(button, {TextColor3 = CurrentTheme.Colors.Primary})
                end)
                
                button.MouseLeave:Connect(function()
                    Utils.Tween(button, {TextColor3 = CurrentTheme.Colors.Text})
                end)
                
                button.MouseButton1Click:Connect(function()
                    Animations.Pulse(button)
                    if config.Callback then
                        config.Callback()
                    end
                end)
                
                button.Parent = self.Container
                
                return {
                    Button = button,
                    SetText = function(text) button.Text = text end,
                    SetVisible = function(visible) button.Visible = visible end
                }
            end
            
            -- ÿØÿßŸÑÿ© ÿ•ŸÜÿ¥ÿßÿ° ŸàÿµŸÅ
            function sectionObj:AddDescription(text)
                local description = Utils.Create("TextLabel", {
                    Name = "Description",
                    Text = text,
                    TextColor3 = CurrentTheme.Colors.SubText,
                    TextSize = 14,
                    Font = Enum.Font.Gotham,
                    TextWrapped = true,
                    BackgroundTransparency = 1,
                    Size = UDim2.new(1, 0, 0, 0),
                    LayoutOrder = #self.Container:GetChildren() + 1
                })
                
                description:GetPropertyChangedSignal("TextBounds"):Connect(function()
                    description.Size = UDim2.new(1, 0, 0, description.TextBounds.Y + 10)
                end)
                
                description.Parent = self.Container
                
                return {
                    Label = description,
                    SetText = function(newText) description.Text = newText end
                }
            end
            
            return sectionObj
        end
        
        table.insert(self.Tabs, tabObj)
        
        return tabObj
    end
    
    return windowObj
end

-- ============================================
-- ÿ™ŸáŸäÿ¶ÿ© ÿßŸÑŸÖŸÉÿ™ÿ®ÿ©
-- ============================================
function UltimateUI.new()
    local self = setmetatable({}, UltimateUI)
    return self
end

local Library = UltimateUI.new()

return Library
